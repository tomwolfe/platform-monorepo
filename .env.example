# Database
DATABASE_URL=

# Ably
ABLY_API_KEY=

# Upstash Redis
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Upstash QStash (for reliable saga execution)
# Get these from https://console.upstash.io/qstash
QSTASH_URL=https://qstash-us-east-1.upstash.io
QSTASH_TOKEN=
# Alternative env name (some SDKs use UPSTASH_QSTASH_TOKEN)
UPSTASH_QSTASH_TOKEN=
# Signing keys for webhook verification (from QStash Console > Keys)
QSTASH_CURRENT_SIGNING_KEY=
QSTASH_NEXT_SIGNING_KEY=

# Clerk
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# Inter-service communication
INTENTION_ENGINE_WEBHOOK_URL=http://localhost:3000/api/webhooks
OPENDELIVER_API_URL=http://localhost:3001/api
TABLESTACK_API_URL=http://localhost:3002/api/v1

# Internal System Security (for recursive self-trigger pattern)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
INTERNAL_SYSTEM_KEY=
NEXT_PUBLIC_APP_URL=http://localhost:3000

# LLM Configuration
LLM_API_KEY=
LLM_BASE_URL=https://api.z.ai/api/paas/v4
LLM_MODEL=glm-4.7-flash

# HuggingFace API (for Semantic Memory Embeddings)
# Get free API key from https://huggingface.co/settings/tokens
# Used for semantic similarity search in vector store
# Model: sentence-transformers/all-MiniLM-L6-v2 (384 dimensions)
HUGGINGFACE_API_KEY=
HUGGINGFACE_MODEL_URL=https://api-inference.huggingface.co/models/sentence-transformers/all-MiniLM-L6-v2

# Upstash Vector (for Production Semantic Memory)
# Optional: If not set, falls back to Redis brute-force search
# Get from https://console.upstash.io/vector
UPSTASH_VECTOR_TOKEN=
UPSTASH_VECTOR_URL=https://vector.upstash.io